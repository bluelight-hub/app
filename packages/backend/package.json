{
  "name": "@bluelight-hub/backend",
  "version": "1.0.0-alpha.16",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "docs:build": "compodoc -p tsconfig.compodoc.json -d docs",
    "docs:serve": "compodoc -p tsconfig.compodoc.json -d docs --serve",
    "docs:cov": "pnpm compodoc --coverageTest -p tsconfig.compodoc.json",
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "seed:einsatz": "pnpm cli seed:einsatz",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage && pnpm docs:cov",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@bluelight-hub/shared": "workspace:*",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/platform-socket.io": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/websockets": "^11.1.1",
    "@prisma/client": "^6.8.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "nanoid": "^5.1.5",
    "nest-commander": "^3.17.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sanitize-filename": "^1.6.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/testing": "^11.1.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.18",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.8.1",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
