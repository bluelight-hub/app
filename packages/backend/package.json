{
  "name": "@bluelight-hub/backend",
  "version": "1.0.0-alpha.21",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "docs:build": "compodoc -c .compodocrc.json -d docs",
    "docs:serve": "compodoc -c .compodocrc.json -d docs --serve",
    "docs:cov": "pnpm compodoc --coverageTest 85 -t -c .compodocrc.json",
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.prisma\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"prisma/**/*.prisma\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "prisma generate && jest",
    "test:watch": "prisma generate && jest --watch",
    "test:cov": "prisma generate && jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "prisma generate && jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "prisma generate && jest --config ./test/jest-e2e.json --watch",
    "test:e2e:cov": "prisma generate && jest --config ./test/jest-e2e.json --coverage",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "check:jsdoc": "ts-node -r tsconfig-paths/register scripts/find-missing-jsdoc.ts",
    "check:jsdoc:public": "ts-node -r tsconfig-paths/register scripts/find-missing-jsdoc.ts --public-only",
    "check:jsdoc:json": "ts-node -r tsconfig-paths/register scripts/find-missing-jsdoc.ts --json-report"
  },
  "dependencies": {
    "@bluelight-hub/shared": "workspace:*",
    "@dotenvx/dotenvx": "^1.48.4",
    "@dotenvx/dotenvx-radar": "^0.14.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.13.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "helmet": "^8.1.0",
    "nanoid": "^5.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "redis": "^5.7.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/js": "^9.32.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.5",
    "@testcontainers/postgresql": "^11.5.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "testcontainers": "^11.5.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "8.38.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
