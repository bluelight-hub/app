{
  "name": "@bluelight-hub/backend",
  "version": "1.0.0-alpha.21",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "docs:build": "compodoc -c .compodocrc.json -d docs",
    "docs:serve": "compodoc -c .compodocrc.json -d docs --serve",
    "docs:cov": "pnpm compodoc --coverageTest 85 -t -c .compodocrc.json",
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.prisma\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"prisma/**/*.prisma\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:threat-rules": "ts-node -r tsconfig-paths/register src/cli-threat-rules.ts",
    "seed:einsatz": "pnpm cli seed:einsatz",
    "seed:admin": "pnpm cli seed:admin",
    "seed:threat-rules": "pnpm cli:threat-rules seed:threat-rules",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage && pnpm docs:cov",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "check:jsdoc": "ts-node -r tsconfig-paths/register scripts/find-missing-jsdoc.ts",
    "check:jsdoc:public": "ts-node -r tsconfig-paths/register scripts/find-missing-jsdoc.ts --public-only",
    "check:jsdoc:json": "ts-node -r tsconfig-paths/register scripts/find-missing-jsdoc.ts --json-report"
  },
  "dependencies": {
    "@bluelight-hub/shared": "workspace:*",
    "@dotenvx/dotenvx": "^1.48.4",
    "@dotenvx/dotenvx-radar": "^0.14.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-socket.io": "^11.1.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/websockets": "^11.1.5",
    "@prisma/client": "^6.13.0",
    "@songkeys/nestjs-redis": "^10.0.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "bullmq": "^5.56.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.1",
    "isbot": "^5.1.29",
    "nanoid": "^5.1.5",
    "nest-commander": "^3.18.0",
    "nodemailer": "^7.0.5",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sanitize-filename": "^1.6.3",
    "ua-parser-js": "^2.0.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/js": "^9.32.0",
    "@nestjs/cli": "^11.0.8",
    "@nestjs/testing": "^11.1.5",
    "@nestjs/throttler": "^6.4.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/bull": "^4.10.4",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "helmet": "^8.1.0",
    "jest": "^30.0.5",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.38.0"
  }
}
